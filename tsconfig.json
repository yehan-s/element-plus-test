{
  "compilerOptions": {
    "target": "ESNext", // 打包目标版本
    "module": "ESNext", // 模块转换的格式
    "esModuleInterop": true, // 允许模块和命名空间导入
    "moduleResolution": "node",
    "skipLibCheck": true, // 跳过类型检查
    "forceConsistentCasingInFileNames": true, // 文件名大小写敏感
    "jsx": "preserve", // 不转换jsx
    "declaration": true, // 生成.d.ts文件
    "sourceMap": true, // 生成.map文件
    "types": ["vue"], // 类型定义文件
  },
}